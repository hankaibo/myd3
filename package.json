{
  "name": "myd3",
  "version": "1.0.0",
  "description": "A d3 webpack kit",
  "keywords": [
    "d3",
    "webpack"
  ],
  "author": "hankaibo",
  "license": "MIT",
  "homepage": "https://github.com/hankaibo/myd3#readme",
  "scripts": {
    "build": "rimraf dist && npm run build:dev",
    "build:dev": "cross-env NODE_ENV=dev webpack --progress --profile --colors",
    "build:prod": "cross-env NODE_ENV=prod webpack --progress --profile --colors",
    "server:dev": "cross-env NODE_ENV=dev webpack-dev-server --open",
    "server:prod": "cross-env NODE_ENV=prod webpack-dev-server --open",
    "start": "npm run server:dev",
    "watch": "npm run wetch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch"
  },
  "dependencies": {
    "d3": "^4.8.0",
    "d3-tip": "^0.7.1",
    "normalize.css": "^6.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.6.1",
    "to-string-loader": "^1.1.5",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hankaibo/myd3.git"
  },
  "bugs": {
    "url": "https://github.com/hankaibo/myd3/issues"
  }
}
